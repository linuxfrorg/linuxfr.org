%main#contents(role="main")
  =h1 "Journal des modifications"

  #changelog.body
    %pre
      = @commits
    %nav
      - if @page > 0
        %span
          = link_to "Commits les plus récents", { page: @page - 1 }, { rel: 'prev' }
      %span
        = link_to "Commits les plus anciens", { page: @page + 1 }, { rel: 'next' }
